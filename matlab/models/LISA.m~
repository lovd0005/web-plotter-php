function [domain, omega] = LIGO_S3(f,numpoints)


xx = load('LISA20pmrtHzPSD.dat');
domain = xx(1,:);
omega = (xx(:,2)/4.1e-22).^2 .* (xx(:,1)/100).^3;
loglog(xx(:,1),omega,'m-','linewidth',2)
text(2e-6,4e-11,'LISA','FontSize',18,'Color','m');


x_start = 69;
x_end = 156;


f = xx(1,:);
left_bound = numpoints +1;
right_bound = 0;

for i = 1 : numpoints
	if  (f(i) >=  x_start)
		left_bound = i;
		break;
	end;
end;

for i = 1 : numpoints
	if  (f(numpoints + 1 -i) <= x_end) 
		right_bound = numpoints + 1 - i;
		break;
	end;
end;

if (right_bound > left_bound)
	domain = f(left_bound:right_bound);
	omega = 0*domain + 8.4e-4;
else
	domain = 0;
	omega = 0;
end;




%LISA from Phinney pages
xx = load('LISA20pmrtHzPSD.dat');
domain = xx(1,:);
omega = (xx(:,2)/4.1e-22).^2 .* (xx(:,1)/100).^3;
loglog(xx(:,1),omega,'m-','linewidth',2)
text(2e-6,4e-11,'LISA','FontSize',18,'Color','m');
