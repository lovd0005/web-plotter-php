function [domain omega] = COBE(f)

x_start = 3e-18;
x_end = 1e-16;
y_start = 1.4e-13*(1e-16/3e-18)^2;
y_end = 1.4e-13;

k = ( y_end - y_start ) / ( x_end - x_start );

size = length(f);
left_bound = size +1;
right_bound = 0;

for i = 1 : size
	if  (f(i) >=  x_start)
		left_bound = i;
		break;
	end
end

for i = 1 : size
	if  (f(size + 1 -i) <= - x_end) 
		right_bound = size + 1 - i;
		break;
	end
end

if (right_bound > = left_bound)
	domain = f(left_bound:right_bound);
	omega(i) = k*(f(i) - x_start) + y_start;
	else
		omega(i) = ' ';
	end