function [domain, omega] = BBN(f,numpoints)

x_start = 1e-10;
x_end = 1e10;

left_bound = numpoints +1;
right_bound = 0;

for i = 1 : numpoints
	if  (f(i) >=  x_start)
		left_bound = i;
		break;
	end;
end;

for i = 1 : numpoints
	if  (f(numpoints + 1 -i) <= x_end) 
		right_bound = numpoints + 1 - i;
		break;
	end;
end;

if (right_bound > left_bound)
	domain = f(left_bound:right_bound);
	omega = 0*domain + 1.5e-5;
else
	domain =[1e-10 1e10];
	omega = 0;
end;

loglog([1e-10 1e10],[1 1]*1.5e-5,'r-','linewidth',3)