function [domain, omega] = LIGO_S3(f,numpoints)

left_bound = numpoints +1;
right_bound = 0;

for i = 1 : numpoints
	if  (f(i) >=  x_start)
		left_bound = i;
		break;
	end;
end;

for i = 1 : numpoints
	if  (f(numpoints + 1 -i) <= x_end) 
		right_bound = numpoints + 1 - i;
		break;
	end;
end;

if (right_bound > left_bound)
	domain = f(left_bound:right_bound);
	omega = 0*domain + 1*2.0e-8/0.72^2;
else
	domain = 0;
	omega = 0;
end;

loglog([69 156],[1 1]*8.4e-4,'k-','linewidth',3)
%text(600,8.4e-4,'LIGO S3','FontSize',18,'Color','k')